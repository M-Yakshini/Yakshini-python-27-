class Solution:
    def wordsTyping(self, s, r, c):
        t=" ".join(s)+" "
        p=0; n=len(t)
        for _ in range(r):
            p+=c
            if t[p%n]==' ':
                p+=1
            else:
                while p>0 and t[(p-1)%n]!=' ':
                    p-=1
        return p//n
