class Trie:
    def __init__(self):
        self.t = {}
    def insert(self, word):
        cur = self.t
        for c in word:
            cur = cur.setdefault(c, {})
        cur["#"] = True
    def search(self, word):
        cur = self.t
        for c in word:
            if c not in cur:
                return False
            cur = cur[c]
        return "#" in cur
    def startsWith(self, prefix):
        cur = self.t
        for c in prefix:
            if c not in cur:
                return False
            cur = cur[c]
        return True
