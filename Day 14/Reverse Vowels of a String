class Solution:
    def reverseVowels(self, s):
        v=set("aeiouAEIOU")
        a=list(s)
        i,j=0,len(a)-1
        while i<j:
            if a[i] not in v: i+=1
            elif a[j] not in v: j-=1
            else:
                a[i],a[j]=a[j],a[i]
                i+=1; j-=1
        return "".join(a)
