class Solution:
    def minMutation(self, s, e, b):
        from collections import deque
        b=set(b)
        q=deque([(s,0)])
        v={s}
        while q:
            x,d=q.popleft()
            if x==e: return d
            for i in range(8):
                for c in "ACGT":
                    y=x[:i]+c+x[i+1:]
                    if y in b and y not in v:
                        v.add(y); q.append((y,d+1))
        return -1
